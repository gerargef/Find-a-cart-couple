{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "templateEngine": "writable",
    "CardField": "writable",
    "requests": "writable"
  },
  "extends": ["eslint:recommended", "prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "rules": {
    "prettier/prettier": "error",
    "camelcase": ["error"],
    "eqeqeq": ["error", "always"],
    "no-unused-vars": "off"
  }
}
